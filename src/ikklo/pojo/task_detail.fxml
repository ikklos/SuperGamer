<AnchorPane prefHeight="350.0" prefWidth="422.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ScrollPane layoutY="55.0" prefHeight="295.0" prefWidth="422.0">
