            <Font size="16.0" />
         </font>
      </Label>
      <Button fx:id="deleteButton" layoutX="353.0" layoutY="19.0" mnemonicParsing="false" onAction="#delete" prefHeight="16.0" prefWidth="43.0" text="删除" />
      <AnchorPane layoutX="95.0" layoutY="3.0" prefHeight="23.0" prefWidth="233.0">
         <children>
            <Label fx:id="taskTitle" alignment="CENTER" layoutX="101.0" layoutY="4.0" text="label" textAlignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
