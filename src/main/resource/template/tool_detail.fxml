<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="408.0" prefWidth="285.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="备注">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ScrollPane hbarPolicy="NEVER" layoutX="14.0" layoutY="50.0" prefHeight="201.0" prefWidth="257.0">
         <content>
            <Label fx:id="noteLabel" maxWidth="250.0" text="没有备注喵&gt;&lt;" wrapText="true">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </content>
      </ScrollPane>
      <Button fx:id="downloadButton" layoutX="50.0" layoutY="357.0" mnemonicParsing="false" onAction="#download" prefHeight="37.0" prefWidth="66.0" text="下载" />
      <TextField fx:id="addressField" disable="true" layoutX="38.0" layoutY="292.0" promptText="请选择下载地址" />
      <Button fx:id="chooseButton" layoutX="199.0" layoutY="292.0" mnemonicParsing="false" onAction="#chooseAddress" text="选择" />
      <Button fx:id="deleteButton" layoutX="173.0" layoutY="357.0" mnemonicParsing="false" onAction="#delete" prefHeight="37.0" prefWidth="66.0" text="删除" />
   </children>
</AnchorPane>
