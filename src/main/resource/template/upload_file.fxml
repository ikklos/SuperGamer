<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="401.0" prefWidth="314.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="fileNameField" layoutX="77.0" layoutY="42.0" promptText="给它起个名吧哥" />
      <TextField fx:id="fileUrlField" disable="true" layoutX="77.0" layoutY="91.0" promptText="选择文件" />
      <TextArea fx:id="noteField" layoutX="58.0" layoutY="181.0" prefHeight="154.0" prefWidth="200.0" promptText="输入备注，注意不要太长" />
      <Button fx:id="uploadButton" layoutX="132.0" layoutY="357.0" mnemonicParsing="false" onAction="#upload" text="上传">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="selectFileButton" layoutX="245.0" layoutY="91.0" mnemonicParsing="false" onAction="#select" prefHeight="23.0" prefWidth="50.0" text="选择" />
      <TextField fx:id="gameNameField" layoutX="78.0" layoutY="135.0" promptText="芝士什么游戏" />
      <Label layoutX="29.0" layoutY="44.0" text="文件名">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="93.0" text="文件路径">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="29.0" layoutY="137.0" text="游戏名">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
